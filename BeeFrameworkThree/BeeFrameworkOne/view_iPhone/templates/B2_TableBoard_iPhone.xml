<?xml version="1.0" encoding="UTF-8"?>

<ui namespace="B2_TableBoard_iPhone">
  <!--
    <row></row>
  -->
  <col class="wrapper">
	<list id="list" orientation="v"/>  <!-- list是一个UIScrollView -->
  </col>

  <style type="text/css">
    .wrapper {
        width: 100%;
        height: 100%;
    }

	#list {
		left: 0px;  <!-- 相当于设置frame时候的  x值  -->
		top: 0px;  <!-- 相当于设置frame时候的  y值  -->
		width: 100%;
		height: 100%;
		insets: 10 0 70 0;   <!--  设置list的属性 UIEdgeInsets -->
	}
	
  </style>
</ui>
